mixin blog(imgURL, imgALT, href)
	+col3
		div.text-left
			img.w-100.img-responsive(src="#{imgURL}", alt="#{imgALT}")
			h4 The Sample Heading Title
			p 27/11/2015
			p Lorem ipsum dolor sit amet, consectetur adipisicing elit. Veniam, neque, do...
			a(href="#{href}") Read more...
			if block
				block
mixin nav-footer-links(h4, a1, a2, a3, a4, hr1, hr2, hr3, hr4)
	.col-xs-12.col-sm-6.col-md-2.col-lg-2
		h4= h4
		nav
			li: a(href=hr1)=a1
			li: a(href=hr2)=a2
			li: a(href=hr3)=a3
			li: a(href=hr4)=a4