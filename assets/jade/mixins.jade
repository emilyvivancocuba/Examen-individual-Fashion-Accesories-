mixin col4
	.col-xs-12.col-sm-4.col-md-4.col-lg-4
		if block
			block
mixin col3
	.col-xs-12.col-sm-6.col-md-3.col-lg-3
		if block
			block
mixin article-price-offer(imgURL, imgALT)
	+col3
		a(href="#")
			img.img-bg-change(src="#{imgURL}", alt="#{imgALT}")
			h4= 'Deliver Wide'
			p
				i.fa.fa-star
				i.fa.fa-star
				i.fa.fa-star
				i.fa.fa-star-o
				i.fa.fa-star-o
			p
				span
					del $242.00
				$218.00
			if block
				block
