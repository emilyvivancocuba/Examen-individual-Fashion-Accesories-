mixin cliente(imgURL, imgALT)
	.col-xs-12.col-sm-4.col-md-2.col-lg-2
		img.w-100.img-responsive(src="#{imgURL}", alt="#{imgALT}")
		if block
			block
mixin tag-new
	.tag-new
		span NEW
		if block
				block
mixin tag-sale
	.tag-sale
		span SALE
		if block
			block
mixin article-price-buy-offer(imgURL, imgALT)
	+col4
		img.img-responsive.img-thumbnail.w-50.text-left(src="#{imgURL}", alt="#{imgALT}")
		div
			h4= 'Deliver Wide'
			p
				span
					del $242.00
				$218.00
			p
				i.fa.fa-star
				i.fa.fa-star
				i.fa.fa-star
				i.fa.fa-star-o
				i.fa.fa-star-o
			a(href="#")
				i.fa.fa-shopping-cart
				span Add to Cart
			if block
				block
